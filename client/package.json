  "version": "1.0.0",
  "description": "",
  "main": "src/bootstrap.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "live-server www",
    "explore": "source-map-explorer bundle/bundle.js",
    "ngc:prod": "ngc -p tsconfig.prod.json",
    "ngc:dev": "ngc -p tsconfig.json",
    "rollup:prod": "rollup -c rollup.config.prod.js",
    "rollup:ie11": "rollup -c rollup.config.prod-ie11.js",
    "rollup:dev": "rollup -c rollup.config.dev.js",
    "copy": "copyfiles -u 1 src/**/*.css src/**/*.html dist/",
    "copy:materialize": "copyfiles -u 3 node_modules/materialize-css/dist/**/*.* www/materialize",
    "prod": "npm run ngc:prod && npm run copy && npm run copy:materialize && npm run rollup:prod",
    "dev": "npm run ngc:dev && npm run copy && npm run copy:materialize && npm run rollup:dev",
    "ie11": "npm run ngc:prod && npm run copy &&  npm run copy:materialize  &&  npm run rollup:ie11",
    "gzip": "gzip -f9k bundle/bundle-ie11.min.js && gzip -f9k bundle/bundle.min.js",
    "build": "npm run dev && npm run prod && npm run ie11 && npm run gzip"
  },
  "dependencies": {
    "@angular/animations": "^4.3.0",
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/router": "^4.3.0",
    "animate.css": "^3.5.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "ng2-toastr": "^4.1.2",
    "ng2-ui-auth": "^7.0.2",
    "rxjs": "^5.4.2",
    "ts-helpers": "^1.1.2",
    "zone.js": "^0.8.13"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.3.0",
    "copyfiles": "^1.2.0",
    "cssnano": "^4.0.0-rc.1",
    "live-server": "^1.2.0",
    "rollup": "0.45.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve-angular": "^2.0.3",
    "rollup-plugin-postcss": "^0.5.3",
    "rollup-plugin-scss": "^0.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "source-map-explorer": "^1.4.0",
    "typescript": "2.4.1",
    "uglify-es": "^3.0.25"
  }
}
